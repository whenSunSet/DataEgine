<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <!-- toolbar（actionbar）颜色 -->
        <item name="colorPrimary">@color/colorAccent</item>
        <!-- 状态栏颜色 -->
        <item name="colorPrimaryDark">@color/colorAccent</item>
        <!-- 窗口的背景颜色 -->
        <item name="android:windowBackground">@color/white</item>
        <!-- SearchView -->
    </style>

    <style name="TabView">
        <item name="android:clickable">true</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="linearLayout_match_wrap_vertical" >
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="linearLayout_match_wrap_horizontal" parent="linearLayout_match_wrap_vertical">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="linearLayout_match_match_horizontal" parent="linearLayout_match_wrap_horizontal">
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="linearLayout_match_match_vertical" parent="linearLayout_match_wrap_vertical">
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="linearLayout_wrap_wrap_vertical" parent="linearLayout_match_wrap_vertical">
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="button_wrap_wrap" >
        <item name="android:textSize">20dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="text_view_wrap_wrap_black_15dp" parent="button_wrap_wrap" >
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15dp</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="text_view_one" >
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:gravity">right|center</item>
    </style>

    <style name="view_10" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:background">@color/blue</item>
    </style>

    <style name="view_2" parent="view_10">
        <item name="android:layout_height">2dp</item>
    </style>

    <style name="view_10_no" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
    </style>

    <style name="view_2_no" parent="view_10_no">
        <item name="android:layout_height">2dp</item>
    </style>

    <style name="text_view_wrap_wrap_white_15dp" parent="text_view_wrap_wrap_black_15dp" >
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="text_view_match_wrap_center_white_15dp" parent="text_view_wrap_wrap_white_15dp" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="text_view_match_wrap_center_black_15dp" parent="text_view_match_wrap_center_white_15dp" >
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text_view_wrap_match_center_white_15dp" parent="text_view_wrap_wrap_white_15dp" >
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="text_view_wrap_match_center_black_15dp" parent="text_view_wrap_match_center_white_15dp" >
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text_view_match_match_center_black_15dp" parent="text_view_wrap_match_center_white_15dp" >
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="text_view_match_match_center_white_15dp" parent="text_view_wrap_match_center_white_15dp" >
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="text_view_match_wrap_left_white_15dp" parent="text_view_wrap_wrap_white_15dp" >
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="text_view_match_wrap_left_black_15dp" parent="text_view_wrap_wrap_white_15dp" >
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">left</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="image_view_wrap_wrap">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="image_view_match_match">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
    </style>


    <style name="register_linear" parent="linearLayout_match_wrap_horizontal">
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">50dp</item>
    </style>

    <style name="register_text_view" parent="text_view_wrap_match_center_black_15dp">
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">110dp</item>
    </style>

    <style name="register_edit_text">
        <item name="android:textSize">15dp</item>
        <item name="android:textColorHint">@color/gray</item>
        <item name="android:text"/>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="app_relative" parent="linearLayout_match_wrap_horizontal">
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>

    <style name="app_image_view">
        <item name="android:padding">10dp</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">60dp</item>
    </style>


    <style name="app_text_view" parent="text_view_wrap_match_center_black_15dp">
        <item name="android:textSize">17dp</item>
        <item name="android:gravity">center|left</item>
    </style>

    <style name="app_into_image_view">
        <item name="android:src">@drawable/into</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">10dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="app_frame">
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="login_button">
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="more_dialog_button" parent="login_button">
        <item name="android:layout_width">200dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="message_relative">
        <item name="android:clickable">true</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">71dp</item>
    </style>

    <style name="message_image">
        <item name="android:padding">10dp</item>
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">70dp</item>
    </style>

    <style name="message_text_view">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/more_gray</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="message_text_view_two" parent="@style/text_view_match_match_center_black_15dp">
        <item name="android:textSize">15dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="message_text_view_three" parent="text_view_match_match_center_black_15dp">
        <item name="android:lines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">left|top</item>
        <item name="android:textColor">@color/more_gray</item>
        <item name="android:layout_height">30dp</item>
    </style>

    <style name="message_frame">
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
    </style>

    <style name="message_image_two">
        <item name="android:padding">3dp</item>
        <item name="android:src">@drawable/circle</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
    </style>

    <style name="message_text_view_four">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
    </style>

    <style name="message_view">
        <item name="android:background">@color/gray</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="me_image">
        <item name="android:padding">15dp</item>
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">60dp</item>
    </style>

    <style name="me_text_view" parent="text_view_match_match_center_black_15dp">
        <item name="android:gravity">left|center</item>
    </style>

    <style name="me_frame">
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="calendar_text_view">
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="calendar_view">
        <item name="android:background">@color/colorAccent</item>
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>


    <style name="sign_in_card_view">
        <item name="android:background">@color/white</item>
        <!--<item name="android:clickable">true</item>-->
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="sign_in_text_view" parent="text_view_match_match_center_black_15dp">
        <item name="android:clickable">true</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="send_graceful_image">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>

    <style name="graceful_item_image" parent="send_graceful_image">
        <item name="android:layout_width">110dp</item>
        <item name="android:layout_height">110dp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>

    <style name="graceful_item_view_pager_cricle_image">
        <item name="android:padding">5dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">20dp</item>
    </style>

</resources>
